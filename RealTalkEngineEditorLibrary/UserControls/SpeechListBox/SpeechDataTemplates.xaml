<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:alexa="clr-namespace:RealTalkEngine.Alexa;assembly=RealTalkEngineRetargeted"
                    xmlns:user_controls="clr-namespace:RealTalkEngine.UserControls;assembly=RealTalkEngineEditorLibrary">
    <user_controls:SpeechDataTemplate x:Key="StringSpeechDataTemplate" DataType="{x:Type alexa:ISsml}">
        <TextBlock Text="{Binding Value}"/>
    </user_controls:SpeechDataTemplate>
    <user_controls:SpeechDataTemplate x:Key="SentenceDataTemplate" DataType="{x:Type alexa:Sentence}">
        <TextBlock Text="{Binding Text}"/>
    </user_controls:SpeechDataTemplate>
</ResourceDictionary>