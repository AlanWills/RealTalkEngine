<editor:Editor x:Class="RealTalkEngineEditorLibrary.Editors.StoryEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:nodenetwork="clr-namespace:NodeNetwork.Views;assembly=NodeNetwork"
             xmlns:editor="clr-namespace:CelesteEngineEditor.Editors;assembly=CelesteEngineEditorLibrary"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <nodenetwork:NetworkView x:Name="Network" Grid.Column="0" ViewModel="{Binding Network}"/>
    </Grid>
</editor:Editor>
